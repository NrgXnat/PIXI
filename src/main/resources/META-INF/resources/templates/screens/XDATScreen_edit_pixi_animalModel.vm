#set($months = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"])
#set($days = [ 1..31 ])
#set($years = [ $!turbineUtils.getYear()..1900])
#if ($data.message)
<font color="red" size="3">$data.message</font>
#end
<p>
<form ID="form1" name="form1" method="post" action="$link.setAction("ModifySubjectAssessorData")">
<input type="hidden" name="project" value="$!{project}" >
#if($vr)
	<font color="red">Invalid parameters:<BR>$vr.toHTML()</font>
<HR>
#end

<TABLE width="100%">
	<TR>
		<TD>
			<table width="100%">
				<TR>
					<TD align="left" valign="middle">
						<DIV class="edit_title">AnimalModel Details</DIV>
					</TD>
				</TR>
			</TABLE>
		</TD>
	</TR>
	<TR>
		<TD>
			<TABLE width="100%">
				<TR>
					<TD valign="top">
						<TABLE>
							<TR><TD colspan='2'>
								#parse("/screens/xnat_edit_subjectAssessorData.vm")
							</TD></TR>
							<tr>
								<TD colspan="2">#parse("/screens/EditProjectSpecificFields.vm")</TD>
							</tr>
							<TR><TD>***PDX</TD><TD>#xdatStringBox("pixi:animalModel/pdxID" $item "" $vr)</TD></TR>
							<TR><TD>Mouse Strain</TD><TD>#xdatStringBox("pixi:animalModel/strain" $item "" $vr)</TD></TR>
							<TR><TD>Mouse Source</TD><TD>#xdatStringBox("pixi:animalModel/source" $item "" $vr)</TD></TR>
							<TR><TD>***Date Weaned</TD><TD>#xdatDateBox("pixi:animalModel/dateWeaned" $item $vr $years)</TD></TR>
							<TR><TD>***Litter</TD><TD>#xdatStringBox("pixi:animalModel/litter" $item "" $vr)</TD></TR>
							<TR><TD>Passage</TD><TD>#xdatStringBox("pixi:animalModel/passage" $item "" $vr)</TD></TR>
							<TR><TD>Immune System Humanized?</TD><TD>#xdatBooleanRadioYesNo("pixi:animalModel/isImmuneSystemHumanized" $item false $vr)</TD></TR>
							<TR><TD>Humanization Type</TD><TD>#xdatStringBox("pixi:animalModel/humanizationType" $item "" $vr)</TD></TR>
							<TR><TD>Tumor Preparation</TD><TD>#xdatStringBox("pixi:animalModel/tumorPreparation" $item "" $vr)</TD></TR>
							#set($tumorInjectionTypes = ["Subcutaneous","Orthotopic"])
							<TR><TD>Tumor Injection Type</TD><TD>#xdatSimpleSelectBoxLbl("Select" "pixi:animalModel/tumorInjectionType" $item $tumorInjectionTypes $vr)</TD></TR>
							<TR><TD>Tumor Injection Site</TD><TD>#xdatStringBox("pixi:animalModel/tumorInjectionSite" $item "" $vr)</TD></TR>
							<TR><TD>Tumor Injection Date/Time</TD><TD>#xdatStringBox("pixi:animalModel/tumorInjectionSite" $item "" $vr)</TD></TR>
							<TR><TD>Engraftment Treatment</TD><TD>#xdatStringBox("pixi:animalModel/engraftmentTreatment" $item "" $vr)</TD></TR>
						</TABLE>
					</TD>
				</TR>
			</TABLE>
		</TD>
	</TR>
	<TR>
		<TD>
			#xdatEditProps($item $edit_screen)
	<TR><TD COLSPAN=2 ALIGN=left><input type="button" ONCLICK="validateForm();"  name="eventSubmit_doInsert" value="Submit"/></TD></TR>
	</TD>
	</TR>
</TABLE>
</form>

